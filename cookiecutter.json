{
  "project_name": "My Awesome Sixie Project",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
  "project_dash": "{{ cookiecutter.project_name.lower()|replace(' ', '-')|replace('_', '-')|replace('.', '-')|trim() }}",
  "description": "Behold My Awesome Project!",
  "author_name": "Joe Sixie",
  "domain_name": "sixfeetup.com",
  "email": "{{ cookiecutter.author_name.lower()|replace(' ', '-') }}@example.com",
  "version": "0.1.0",
  "timezone": "US/Eastern",
  "aws_region": "us-east-1",
  "aws_account_id": "000000000000",
  "create_react_frontend": "y",
  "mail_service": [
    "Mailgun",
    "Amazon SES",
    "Other SMTP"
  ],
  "use_drf": "n",
  "custom_bootstrap_compilation": "n",
  "use_compressor": "n",
  "use_celery": "n",
  "debug": "n",
  "source_control_provider": ["bitbucket", "github", "none"],
  "__prompts__": {
    "source_control_provider": {
      "__prompt__": "Which source control provider do you want to use?",
      "bitbucket": "Bitbucket",
      "github": "Github",
      "none": "No source control provider"
    }
  }
}
