{
	"name": "{{cookiecutter.project_name}}",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"//scripts": {
		"prepare": "Use this version of prepare command while working on cookiecutter itself cd ../../ && husky install ./{{cookiecutter.project_slug}}/frontend/.husky"
	},
	"scripts": {
		"dev": "vite",
		"test": "NODE_ENV=test jest",
		"build": "tsc -p tsconfig.prod.json && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .ts,.tsx",
		"lint-staged-fix": "eslint . --ext .ts,.tsx --fix",
		"format-staged": "prettier --write",
		"prepare": "cd ../../ && husky install ./{{cookiecutter.project_slug}}/frontend/.husky"
	},
	"lint-staged": {
		"*.{json,css,md}": "npm run format-staged",
		"*.{js,jsx,ts,tsx}": [
			"npm run format-staged",
			"npm run lint-staged-fix"
		]
	},
	"dependencies": {
		"@tanstack/react-query": "4.20.9",
		"axios": "1.2.2",
		"localforage": "1.10.0",
		"match-sorter": "6.3.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.6.1",
		"sort-by": "1.2.0"
	},
	"devDependencies": {
		"@swc/core": "1.3.24",
		"@swc/jest": "0.2.24",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "14.4.3",
		"@types/node": "18.11.18",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.9",
		"@typescript-eslint/eslint-plugin": "5.48.0",
		"@typescript-eslint/parser": "5.48.0",
		"@vitejs/plugin-react": "3.0.0",
		"camelcase": "6.3.0",
		"eslint": "8.31.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-react": "7.31.11",
		"husky": "8.0.3",
		"jest": "29.3.1",
		"jest-environment-jsdom": "29.3.1",
		"jest-watch-typeahead": "2.2.1",
		"lint-staged": "13.1.0",
		"prettier": "2.8.1",
		"typescript": "4.9.3",
		"vite": "4.0.0",
		"vite-tsconfig-paths": "4.0.3"
	}
}
